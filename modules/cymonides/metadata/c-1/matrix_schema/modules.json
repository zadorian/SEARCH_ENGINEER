    "corporella": {
      "display_name": "Corporella - Company Intelligence",
      "location": "python-backend/modules/corporella/",
      "main_file": "module.py",
      "suite": "corporella",
      "friction": "Open/Paywalled",
      "description": "Company registry data, officers, beneficial owners, shareholders, and corporate structure.",
      "inputs": {
        "primary": [
          {
            "code": 13,
            "name": "company_name",
            "type": "company",
            "required": true
          },
          {
            "code": 14,
            "name": "company_reg_id",
            "type": "company",
            "required": false
          },
          {
            "code": 15,
            "name": "company_vat_id",
            "type": "company",
            "required": false
          },
          {
            "code": 16,
            "name": "company_previous_name",
            "type": "company",
            "required": false
          }
        ]
      },
      "outputs": [
        {
          "codes": [42, 43, 44, 45, 46, 47, 48, 49, 50],
          "node_types": ["company"],
          "description": "Complete company profile with registration details"
        },
        {
          "codes": [51, 52, 53, 54, 55, 56, 57, 58],
          "node_types": ["person"],
          "edge_types": ["officer_of", "director_of", "shareholder_of"],
          "description": "Company officers, directors, and shareholders"
        },
        {
          "codes": [59, 60, 61, 62, 63],
          "node_types": ["person"],
          "edge_types": ["beneficial_owner_of"],
          "description": "Beneficial owners and UBO data"
        }
      ],
      "data_sources": [
        {
          "name": "OpenCorporates",
          "type": "api",
          "coverage": "global",
          "friction": "freemium"
        },
        {
          "name": "Companies House (UK)",
          "type": "api",
          "coverage": "GB",
          "friction": "open"
        },
        {
          "name": "Aleph Collections",
          "type": "dataset",
          "coverage": "multi-country",
          "friction": "open"
        },
        {
          "name": "OpenOwnership",
          "type": "api",
          "coverage": "global",
          "friction": "open"
        }
      ],
      "resource_files": [
        "resources/opencorporates.py",
        "resources/companies_house.py",
        "resources/aleph.py",
        "resources/open_ownership.py"
      ]
