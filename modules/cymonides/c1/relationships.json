{
  "_meta": {
    "version": "3.2.1",
    "title": "NEXUS Atomic Relationship Ontology",
    "philosophy": "Atomic particles + hierarchy. Let patterns emerge from the graph, don't bake them into edge types.",
    "canonical_location": "/data/SEARCH_ENGINEER/BACKEND/modules/cymonides/c1/relationships.json",
    "last_updated": "2026-01-13",
    "changelog": {
      "3.2.1": [
        "CONSOLIDATED: Moved to cymonides/c1/ as canonical single source of truth",
        "Symlink created at /data/CLASSES/NEXUS/RELATIONSHIPS/ontology.json for backward compatibility"
      ],
      "3.2.0": [
        "Added enrichment_of root relationship (code 16) for NEXUS enrichment queries (Discovery = NEXUS enrichment of unknowns)",
        "Added subtypes: resolved_to (161), rejected_as (162), candidate_of (163), sourced_from (164), discovered_via (165)",
        "Added node types: enrichment_profile, enrichment_candidate",
        "Added alias mappings for enrichment relationships"
      ],
      "3.1.1": [
        "FIXED: Converted jurisdictional officer terms to aliases of director_of (consejero_of, syndic_of, gerant_of)",
        "FIXED: Moved German KG partnership roles from officer_of to partner_of subtypes (general_partner_of, limited_partner_of)",
        "Cleaner taxonomy: aliases for equivalent concepts, correct legal classification (partners ≠ officers)"
      ],
      "3.1.0": [
        "Added regulated_by root (code 14) for regulatory relationships",
        "Added not_same_as (code 15) for entity resolution",
        "Added commercial relationship subtypes: supplies_to (23), customer_of (24), competitor_of (25)",
        "Expanded alias coverage (70+ new mappings from Aleph, ICIJ, PSC, EU registers)"
      ]
    },
    "design_principles": [
      "1. ATOMIC: ~14 fundamental relationship types (particles)",
      "2. HIERARCHICAL: Subtypes inherit from parents (officer_of → director_of)",
      "3. ALIASES: External system mappings (ftm:Directorship → officer_of)",
      "4. EMERGENT: Complex patterns emerge from graph traversal, not special edge types",
      "5. ALWAYS VALID: Parent type is always acceptable (officer_of works even if you know director_of)"
    ],
    "structure": {
      "value": "Canonical relationship name (snake_case)",
      "aliases": "Alternative names from external systems",
      "subtypes": "More specific children that inherit from parent"
    }
  },

  "relationships": {

    "same_as": {
      "code": 1,
      "description": "Two nodes represent the same real-world entity (identity/deduplication)",
      "source_types": ["*"],
      "target_types": ["*"],
      "inverse": "same_as",
      "bidirectional": true,
      "aliases": ["ftm:SameAs", "owl:sameAs", "skos:exactMatch"],
      "metadata": {
        "match_type": ["identifier", "name_exact", "name_fuzzy", "multi_criteria", "manual"],
        "confidence": "float 0-1"
      },
      "subtypes": {
        "not_same_as": {
          "code": 15,
          "description": "Explicitly different entities (negative entity resolution). Critical for preventing false positives.",
          "inverse": "not_same_as",
          "bidirectional": true,
          "aliases": ["different_from", "distinct_from", "owl:differentFrom"],
          "metadata": {
            "reason": ["different_jurisdiction", "different_birth_date", "different_identifier", "manual_review"],
            "confidence": "float 0-1"
          },
          "note": "Negative evidence is as valuable as positive. Person A ≠ Person B prevents incorrect merging."
        }
      }
    },

