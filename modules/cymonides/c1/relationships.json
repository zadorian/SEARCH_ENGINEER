      "target_types": ["*"],
      "inverse": "enriched_by",
      "bidirectional": false,
      "aliases": ["discovery_of", "search_for"],
      "metadata": {
        "confidence": "float 0-1",
        "method": "string",
        "source_name": "string"
      },
      "subtypes": {
        "resolved_to": {
          "code": 161,
          "description": "Enrichment query resolved to selected entity (query → selected candidate)",
          "source_types": ["enrichment_profile"],
          "target_types": ["person", "company", "organization", "email", "phone", "username", "vehicle", "vessel", "aircraft", "domain"],
          "inverse": "resolved_from",
          "aliases": ["discovered_as", "identified_as"],
          "metadata": {
            "resolution_time": "datetime",
            "candidate_id": "string",
            "confidence": "float 0-1"
          },
          "note": "Creates concrete SUBJECT entity from enrichment candidate selection"
        },
        "rejected_as": {
          "code": 162,
          "description": "Enrichment query rejected candidate (negative match)",
          "source_types": ["enrichment_profile"],
          "target_types": ["enrichment_candidate"],
          "inverse": "rejected_from",
          "aliases": ["not_match", "excluded"],
          "metadata": {
            "rejection_reason": "string",
            "reviewer": "string"
          },
          "note": "Negative evidence prevents future false positives"
        },
        "candidate_of": {
          "code": 163,
          "description": "Entity is potential match for enrichment query",
          "source_types": ["enrichment_candidate"],
          "target_types": ["enrichment_profile"],
          "inverse": "has_candidate",
          "aliases": ["potential_match_for", "suggested_for"],
          "metadata": {
            "confidence": "float 0-1",
            "rank": "integer",
            "source": "string"
          }
        },
        "sourced_from": {
          "code": 164,
          "description": "Candidate data retrieved from external source",
          "source_types": ["enrichment_candidate"],
          "target_types": ["source"],
          "inverse": "provided",
          "aliases": ["fetched_from", "retrieved_from"],
          "metadata": {
            "source_type": ["api", "database", "scrape", "manual"],
            "fetch_time": "datetime"
          }
        },
        "discovered_via": {
          "code": 165,
          "description": "Entity discovered through enrichment method",
          "source_types": ["person", "company", "organization", "email", "phone", "username", "vehicle", "vessel", "aircraft", "domain"],
          "target_types": ["enrichment_profile"],
          "inverse": "discovered",
          "aliases": ["found_via", "identified_via"],
