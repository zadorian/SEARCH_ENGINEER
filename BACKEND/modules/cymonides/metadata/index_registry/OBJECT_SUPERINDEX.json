  "query_strategies": {
    "term": {
      "description": "Exact match (email, phone, company_number)",
      "example": {"term": {"email": "user@example.com"}}
    },
    "match": {
      "description": "Full-text match with analysis (names, addresses)",
      "example": {"match": {"name": {"query": "John Smith", "fuzziness": "AUTO"}}}
    },
    "wildcard": {
      "description": "Pattern matching (URLs with specific patterns)",
      "example": {"wildcard": {"url": "*linkedin.com*"}}
    },
    "nested": {
      "description": "Query nested objects (officers array)",
      "example": {"nested": {"path": "officers", "query": {"match": {"officers.name": "John"}}}}
    }
  }
