# CYMONIDES Agent - Environment Variables
# Copy to .env and fill in your values

# =============================================================================
# REQUIRED: Claude API
# =============================================================================
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# =============================================================================
# REQUIRED: Elasticsearch
# =============================================================================
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
# Optional: If using authentication
# ELASTICSEARCH_USER=elastic
# ELASTICSEARCH_PASSWORD=your-password-here

# =============================================================================
# OPTIONAL: Agent Configuration
# =============================================================================
# Default model for the agent
CYMONIDES_MODEL=claude-sonnet-4-20250514

# Default project ID
CYMONIDES_PROJECT_ID=default

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# =============================================================================
# OPTIONAL: Index Configuration
# =============================================================================
# Default batch size for indexing
INDEX_BATCH_SIZE=1000

# Test batch size
TEST_BATCH_SIZE=100
