{
  "source_id": "cc-pdf-2025",
  "source_name": "CommonCrawl PDF 2025",
  "target_index": "cymonides-2",
  "created": "2025-01-02",
  "last_updated": "2025-01-02",

  "fundamental_rules": {
    "rule_1": "EVERY URL gets indexed - NO EXCEPTIONS",
    "rule_2": "Exclusion = demotion to Tier 3, NOT skipping",
    "rule_3": "Content patterns ALWAYS override URL patterns",
    "rule_4": "Red flag tripwires = automatic Tier 1 + flagged",
    "rule_5": "Context extraction = 5 words before + 5 words after"
  },

  "tiers": {
    "1_FULL": {
      "description": "URL + Content (full-text indexed) + embeddings + concept extraction + temporal + spatial + entity extraction with 5-word context"
    },
    "2_EXTRACT": {
      "description": "URL + Extracted metadata - NO full-text keyword index, NO embeddings stored"
    },
    "3_URL_ONLY": {
      "description": "URL + available metadata only - no extraction, no content"
    }
  },

  "context_extraction": {
    "words_before": 5,
    "words_after": 5
  },

  "tier_1_triggers": {
    "red_flag_tripwires": {
      "description": "Any match promotes to Tier 1 + flags document",
      "indices": [
        {
          "index": "red_flag",
          "docs": 1950406,
          "ram_estimate": "~1GB",
          "note": "Consolidated sanctions, PEPs, adverse media, ICIJ, enforcement"
        },
        {
          "index": "wdc-localbusiness-entities",
          "docs": 478341,
          "ram_estimate": "~250MB",
          "note": "Schema.org LocalBusiness entities"
        },
        {
          "index": "wdc-governmentorganization-entities",
          "docs": 25229,
          "ram_estimate": "~15MB",
          "note": "Government organizations"
        }
      ],
      "total_tripwire_entities": 2453976,
      "total_ram_estimate": "~1.3GB"
    },

    "registry_domains": [
      "sec.gov", "edgar-online.com", "edgarfilings.com",
      "companieshouse.gov.uk", "find-and-update.company-information.service.gov.uk",
      "e-justice.europa.eu",
      "nyse.com", "nasdaq.com", "londonstockexchange.com", "lse.co.uk",
      "euronext.com", "deutsche-boerse.com", "xetra.com", "six-group.com", "borsaitaliana.it",
      "e-cegjegyzek.hu", "sudregister.hrsr.sk", "firmen.wko.at",
      "handelsregister.de", "unternehmensregister.de",
      "infogreffe.fr", "societe.com", "kvk.nl", "opencorporates.com",
      "proff.no", "proff.se", "proff.dk", "bisnode.com", "dun-bradstreet.com",
      "bvicompanyregistry.vg", "cayman.gov.ky", "jerseyfsco.je",
      "guernseyregistry.com", "iomcompanies.com",
      "egrul.nalog.ru", "e-gov.kz"
    ],

    "content_patterns": {
      "annual_reports": {
        "description": "Multilingual annual report detection",
        "patterns": [
          "annual report", "10-K", "10K", "20-F", "20F",
          "yearly report", "geschäftsbericht", "rapport annuel",
          "informe anual", "relazione annuale", "éves jelentés"
        ]
      },
      "legal_entity_ids": {
        "description": "Company registration numbers by country",
        "types": [
          "LEI", "UK_CRN", "DE_HRB", "SEC_CIK", "FR_SIREN", "FR_SIRET",
          "NL_KVK", "BE_BCE", "CH_UID", "AT_FN", "IT_CF", "IT_PIVA",
          "ES_CIF", "PL_REGON", "PL_NIP", "PL_KRS", "CZ_ICO",
          "HU_CEGJSZ", "HR_OIB", "DUNS", "ISIN"
        ]
      },
      "aircraft_tail_numbers": {
        "description": "Aircraft registration by country",
        "types": [
          "US_FAA (N-numbers)", "UK_CAA (G-XXXX)", "DE_LBA (D-XXXX)",
          "FR_DGAC (F-XXXX)", "CA_TC (C-XXXX)", "AU_CASA (VH-XXX)",
          "CH_FOCA (HB-XXX)", "NL_ILT (PH-XXX)", "IT_ENAC (I-XXXX)",
          "ES_AESA (EC-XXX)", "AT_ACG (OE-XXX)", "BE_BCAA (OO-XXX)",
          "IE_IAA (EI-XXX)", "RU_FATA (RA-XXXXX)", "CN_CAAC (B-XXXX)"
        ]
      },
      "vessel_numbers": {
        "types": ["IMO (7 digits)", "MMSI (9 digits)"]
      },
      "court_case_numbers": {
        "types": ["US_FED", "UK_NEUTRAL", "EU_CASE"]
      },
      "bank_accounts": {
        "types": ["IBAN", "SWIFT"]
      },
      "crypto_addresses": {
        "types": ["BTC", "BTC_BECH32", "ETH"]
      }
    },

    "corporate_url_keywords": [
      "annual-report", "annualreport", "annual_report",
      "financial-statement", "financial_statement", "financials",
      "10-k", "10k", "20-f", "20f", "6-k", "8-k",
      "quarterly-report", "q1", "q2", "q3", "q4",
      "earnings", "investor-relations", "ir",
      "investor", "shareholders", "proxy", "prospectus",
      "offering", "ipo", "bond", "debt",
      "regulatory", "compliance", "filing", "disclosure",
      "registration", "form-", "sec-filing",
      "agreement", "contract", "terms", "bylaws",
      "articles-of", "certificate-of", "charter",
      "litigation", "settlement", "court",
      "governance", "board", "management", "executive",
      "compensation", "directors", "officers",
      "corporate-structure", "org-chart", "subsidiary",
      "sustainability", "esg", "csr", "environmental",
      "social-responsibility", "climate", "carbon"
    ],

    "legal_suffixes_in_domain": [
      "ltd", "llc", "inc", "corp", "plc",
      "gmbh", "ag", "kg", "ohg", "ug",
      "sa", "sas", "sarl", "srl", "sl",
      "bv", "nv", "ab", "as", "oy", "oyj",
      "sp", "kft", "zrt", "nyrt", "bt",
      "doo", "dd", "ad", "ood", "eood",
      "jsc", "pjsc", "ojsc"
    ],

    "domain_list_overrides": {
      "linkedin_companies": {
        "index": "affiliate_linkedin_companies",
        "field": "domain",
        "effect": "promote to Tier 1"
      },
      "wdc_schema": {
        "index": "wdc_localbusiness",
        "field": "domain",
        "effect": "promote to Tier 1"
      }
    }
  },

  "tier_2_inclusions": {
    "categories": ["gambling", "crypto"],
    "note": "Extraction but no full-text/embeddings"
  },

  "tier_3_demotions": {
    "non_allowed_tlds": {
      "description": "Demote unless overridden by LinkedIn/WDC",
      "allowed_tlds": [
        "org", "com",
        "fr", "de", "it", "hu", "co.uk",
        "ee", "lt", "lv",
        "ru", "kz", "ua",
        "ca",
        "gov", "gov.uk"
      ],
      "explicitly_excluded": ["edu", "ac.uk", "edu.au", "ac.at", "edu.cn"]
    },

    "majestic_million": {
      "description": "Demote unless on LinkedIn list",
      "overridable_by": ["linkedin_companies"]
    },

    "free_hosting_platforms": [
      "blogspot.com", "wordpress.com", "wix.com", "weebly.com",
      "squarespace.com", "tumblr.com", "medium.com", "substack.com",
      "github.io", "gitlab.io", "netlify.app", "vercel.app",
      "herokuapp.com", "firebaseapp.com", "web.app",
      "sites.google.com", "docs.google.com"
    ],

    "ut1_blacklist_categories": ["adult", "malware"]
  },

  "priority_order": [
    "1. Red flag tripwires → Tier 1",
    "2. Registry domains → Tier 1",
    "3. Content patterns (LEI, tail numbers, etc.) → Tier 1",
    "4. Corporate URL keywords → Tier 1",
    "5. Legal suffix in domain → Tier 1",
    "6. LinkedIn/WDC domain → Tier 1",
    "7. Majestic Million (no override) → Tier 3",
    "8. Non-allowed TLD (no override) → Tier 3",
    "9. Free hosting → Tier 3",
    "10. Default → Tier 2"
  ],

  "notes": [
    "Content patterns ALWAYS override URL-based demotion",
    "A document on blogspot.com with an LEI → Tier 1",
    "News sites are INCLUDED (not excluded)",
    "Forums/blogs are NOT excluded - tier depends on content"
  ]
}
