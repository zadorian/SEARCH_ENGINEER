  "assignment_rules": {
    "description": "Rules for automatically assigning observation codes to jurisdictions",
    "transparency_assignment": {
      "HIGH": "friction_profile.open >= 0.8 AND data_availability.accessible.length >= 4",
      "MEDIUM": "friction_profile.open >= 0.5 AND friction_profile.open < 0.8",
      "LOW": "friction_profile.open < 0.5 AND friction_profile.restricted > 0.2",
      "OPAQUE": "dead_ends contains permanent=true for beneficial_ownership OR legal_notes contains 'no transparency'"
    },
    "registry_assignment": {
      "PUBLIC": "wiki_sections.corporate_registry contains (pub) markers AND no payment required",
      "PAID": "wiki_sections contains payment/purchase mentions OR friction_profile.paywalled > 0.05",
      "LIMITED": "dead_ends.reason_category contains 'registry_limitation'"
    },
    "language_assignment": {
      "CYRILLIC": "jurisdiction_code IN ['RU', 'KZ', 'AZ', 'AM', 'BY', 'UA', 'KG', 'TJ', 'UZ', 'TM', 'MN', 'BG', 'RS', 'ME', 'MK', 'BA']",
      "ARABIC": "jurisdiction_code IN ['AE', 'SA', 'EG', 'IQ', 'SY', 'JO', 'LB', 'KW', 'QA', 'BH', 'OM', 'YE', 'LY', 'TN', 'DZ', 'MA']"
    }
  },
