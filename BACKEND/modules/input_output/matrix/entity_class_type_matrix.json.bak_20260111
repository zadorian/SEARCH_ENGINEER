  "_meta": {
    "version": "2.0.0",
    "title": "I/O Entity Class/Type Matrix",
    "description": "THE CENTRAL REGISTRY for all node classes and types. Hierarchy: CLASS > TYPE > SUBTYPE.",
    "canonical_path": "/data/INPUT_OUTPUT/matrix/entity_class_type_matrix.json",
    "last_updated": "2026-01-11",
    "related_files": {
      "types": "/data/INPUT_OUTPUT/matrix/types.json",
      "relationships": "/data/INPUT_OUTPUT/matrix/relationships.json",
