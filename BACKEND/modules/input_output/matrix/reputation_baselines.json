  "mismatch_patterns": {
    "description": "Common claim vs evidence mismatches",
    "patterns": [
      {
        "id": "inflated_visibility",
        "claim": "Industry leader / well-known / prominent",
        "evidence_gap": "< 20 unique mentions, no T1/T2 coverage",
        "severity": "HIGH",
        "interpretation": "Claims not supported by media record"
      },
      {
        "id": "ghost_company",
        "claim": "Major corporation / global presence",
        "evidence_gap": "< 50 referring domains, < 10 unique mentions",
        "severity": "HIGH",
        "interpretation": "Digital footprint doesn't match claimed scale"
      },
      {
        "id": "pr_only",
        "claim": "Award-winning / recognized",
        "evidence_gap": "> 90% PR republication, 0 editorial features",
        "severity": "MEDIUM",
        "interpretation": "No third-party validation of claims"
      },
      {
        "id": "sudden_emergence",
        "claim": "20+ years experience",
        "evidence_gap": "First mention < 3 years ago",
        "severity": "MEDIUM",
        "interpretation": "Track record not verifiable pre-recent period"
      },
      {
        "id": "silent_expert",
        "claim": "Thought leader / expert",
        "evidence_gap": "Never quoted by others, all self-authored",
        "severity": "MEDIUM",
        "interpretation": "Expertise not validated by third parties"
      },
      {
        "id": "hidden_adverse",
        "claim": "Clean reputation",
        "evidence_gap": "Coverage gap during known controversy period",
        "severity": "INVESTIGATE",
        "interpretation": "May indicate reputation management / removal"
      }
    ]
  },
  "authenticity_indicators": {
    "high_authenticity": [
      "Multiple T1/T2 sources independently covering same events",
      "Third-party quotes about subject (not just subject's own quotes)",
      "Investigative journalism pieces",
      "Critical/analytical coverage (not just announcements)",
      "Coverage span > 5 years with consistent presence"
    ],
    "low_authenticity": [
      "Same press release text across 10+ outlets",
      "Only company-issued statements republished",
      "Coverage cluster in single week (PR burst)",
      "No third-party validation of claims",
      "Zero negative coverage despite high volume"
    ]
  }
}
