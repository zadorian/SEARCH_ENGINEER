{
  "meta": {
    "description": "Baseline expectations for reputation profiles by entity type",
    "purpose": "Enable comparison of subject profile to 'normal' for their category",
    "updated": "2025-12-29"
  },
  "entity_types": {
    "fortune_500_ceo": {
      "label": "Fortune 500 CEO",
      "description": "Chief executive of major publicly traded corporation",
      "expectations": {
        "mentions": {"min": 500, "max": 5000, "typical": 1500},
        "unique_after_dedup": {"min": 200, "max": 2000, "typical": 600},
        "tier_1_pct": {"min": 20, "max": 50, "typical": 30},
        "tier_2_pct": {"min": 20, "max": 40, "typical": 30},
        "countries_covered": {"min": 3, "max": 20, "typical": 8},
        "languages": {"min": 2, "max": 10, "typical": 4},
        "coverage_span_years": {"min": 5, "max": 30, "typical": 15},
        "original_editorial_pct": {"min": 40, "max": 80, "typical": 55},
        "sentiment_negative_pct": {"min": 5, "max": 30, "typical": 15}
      },
      "red_flags": [
        {"condition": "mentions < 100", "severity": "HIGH", "message": "Far below expected visibility for role"},
        {"condition": "tier_1_pct < 5", "severity": "MEDIUM", "message": "Absence from major publications unusual"},
        {"condition": "pr_pct > 70", "severity": "MEDIUM", "message": "Coverage dominated by PR, limited editorial"}
      ],
      "normal_patterns": [
        "Quarterly earnings coverage",
        "Annual meeting mentions",
        "Industry conference speaking",
        "M&A announcements",
        "Regulatory filings coverage"
      ]
    },
